# Database (PostgreSQL)
DATABASE_URL="postgresql://USER:PASSWORD@HOST:PORT/DATABASE?schema=public"

# NextAuth
NEXTAUTH_SECRET="your-secret-min-32-chars"
NEXTAUTH_URL="http://localhost:3000"
NEXT_PUBLIC_APP_URL="http://localhost:3000"

# Password security
PASSWORD_PEPPER="your-secret-pepper-string"

# Google OAuth
GOOGLE_CLIENT_ID=XXXXX
GOOGLE_CLIENT_SECRET=XXXXX


# Razorpay (key id is public, used in checkout.js)
NEXT_PUBLIC_RAZORPAY_KEY_ID=XXXXX
RAZORPAY_KEY_ID=XXXXX
RAZORPAY_KEY_SECRET=XXXXX
RAZORPAY_WEBHOOK_SECRET=XXXXX

# OpenStreetMap (No API key needed - using Nominatim for reverse geocoding)
# Optional but recommended to avoid Nominatim rate limits
NOMINATIM_USER_AGENT="GymDuo/1.0"
